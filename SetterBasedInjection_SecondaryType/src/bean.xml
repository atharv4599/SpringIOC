<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="s" class="com.main.model.Student">
    <property name="rollno" value="1"></property>
    <property name="name" value="Siddarth"></property>
    <property name="addr" ref="ad"></property>     
    </bean>
    <bean id="ad" class="com.main.model.Address">
      <property name="areaName" value="kharadi"></property>
      <property name="cityName" value="pune"></property>
      <property name="pin" value="411011"></property>
      
    </bean>
    
        
    </beans>